  				</TextView>
  					
  					<ImageButton android:id="@+id/downimage2"  android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/zr_adjust_down_selector" android:layout_marginLeft="10dip" android:layout_centerVertical="true">
	  				</ImageButton>
	  				<EditText android:id="@+id/xpjEdit" android:layout_width="60dip" android:layout_height="wrap_content" android:background="@drawable/center"  android:saveEnabled="true" >
