  		android:layout_marginTop="20dip" >
  	-->
  	<LinearLayout android:id="@+id/quotesetlinearyout" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="20dip" android:layout_weight="1" android:orientation="vertical" android:keepScreenOn="false" android:layout_below="@id/zr_headerBar">
  			
  			<LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" android:background="@drawable/hqyj_edit_bg_01" android:gravity="center_vertical">
