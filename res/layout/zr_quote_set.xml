  				</TextView>
  					
  					<ImageButton android:id="@+id/downimage" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/zr_adjust_down_selector" android:layout_marginLeft="10dip"  android:layout_centerVertical="true">
	  				</ImageButton>
	  				<EditText android:id="@+id/spjEdit" android:layout_width="60dip" android:layout_height="wrap_content" android:background="@drawable/center"  >
	  				</EditText>
	  				<ImageButton  android:id="@+id/upimage" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/zr_adjust_up_selector"  android:layout_centerVertical="true">
	  				</ImageButton >
  				
  			</LinearLayout>
  			
  			<LinearLayout  android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" android:background="@drawable/hqyj_edit_bg_02" android:gravity="center_vertical">
