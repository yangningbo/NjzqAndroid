<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout android:orientation="vertical" android:id="@+id/zrquerystocklayout" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
  
   <include android:id="@+id/zr_headerBar" android:layout_width="fill_parent" android:layout_height="@dimen/title_height" layout="@layout/zr_topbar" />
  
   <LinearLayout android:id="@+id/zrfundtransferscroll" android:orientation="vertical"
		 android:layout_width="fill_parent"  
		 android:layout_height="fill_parent"
		 android:layout_marginTop="30dip" 
		 android:layout_below="@id/zr_headerBar"
		 android:layout_weight="1.0">
   		 <LinearLayout android:id="@+id/zrtip" 
 			   android:background="@drawable/zrtablegroupcellheader" 
 			   android:layout_width="fill_parent" 
 	    	   android:layout_height="wrap_content" 
 	    	   android:paddingTop="50dip">
   		 	<RelativeLayout android:id="@+id/RelativeLayout02"
		     	android:layout_width="fill_parent" android:layout_gravity="center"
				android:layout_height="wrap_content" android:layout_marginLeft="15.0dip"
				android:layout_marginRight="15.0dip">		   
           		<Spinner android:id="@+id/fundcompany" android:background="@drawable/spinner_drawable" 
					android:paddingLeft="80.0dip" android:layout_width="fill_parent"
					android:layout_height="40dip" android:ellipsize="middle" />
				<TextView android:gravity="center_vertical" android:id="@+id/TextView011"
					android:paddingLeft="7.0dip" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:layout_marginRight="15.0dip"
					android:text="@string/fundcompany" android:layout_alignLeft="@id/fundcompany"
					android:layout_alignTop="@id/fundcompany" android:layout_alignBottom="@id/fundcompany"
					style="@style/loginpage_TextView01" 
					android:textSize="18dip" />
			</RelativeLayout>
        </LinearLayout>	
        
        
        
         <LinearLayout android:id="@+id/zrtip3" 
   		 		android:background="@drawable/zrtablegroupcellcenter" 
   		 	    android:layout_width="fill_parent" 
   		 	    android:layout_height="wrap_content">
   		 	<RelativeLayout android:id="@+id/RelativeLayout04"
		     	android:layout_width="fill_parent" android:layout_gravity="center"
				android:layout_height="wrap_content" android:layout_marginLeft="15.0dip"
				 android:layout_marginRight="15.0dip">		   
           		<Spinner android:id="@+id/fundlevel" android:background="@drawable/spinner_drawable" 
					android:paddingLeft="110.0dip" android:layout_width="fill_parent"
					android:layout_height="45dip"
					android:textSize="18dip"  />
				<TextView android:gravity="center_vertical" android:id="@+id/TextView011"
					android:paddingLeft="7.0dip" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:layout_marginRight="15.0dip"
					android:text="@string/fundlevel" android:layout_alignLeft="@id/fundlevel"
					android:layout_alignTop="@id/fundlevel" android:layout_alignBottom="@id/fundlevel"
					style="@style/loginpage_TextView01" 
					android:textSize="18dip"/>
			</RelativeLayout>
        </LinearLayout>	
        <LinearLayout android:id="@+id/zrtip4" 
   		 		android:background="@drawable/zrtablegroupcellcenter" 
   		 	    android:layout_width="fill_parent" 
   		 	    android:layout_height="wrap_content"
   		 	   >
   		 	<RelativeLayout android:id="@+id/RelativeLayout05"
		     	android:layout_width="fill_parent" android:layout_gravity="center"
				android:layout_height="wrap_content" android:layout_marginLeft="15.0dip"
				 android:layout_marginRight="15.0dip">		   
           		<Spinner android:id="@+id/fundmanagerlevel" android:background="@drawable/spinner_drawable" 
					android:paddingLeft="110.0dip" android:layout_width="fill_parent"
					android:layout_height="45dip" 
					android:textSize="18dip" />
				<TextView android:gravity="center_vertical" android:id="@+id/TextView011"
					android:paddingLeft="7.0dip" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:layout_marginRight="15.0dip"
					android:text="@string/fundmanagerlevel" android:layout_alignLeft="@id/fundmanagerlevel"
					android:layout_alignTop="@id/fundmanagerlevel" android:layout_alignBottom="@id/fundmanagerlevel"
					style="@style/loginpage_TextView01" 
					android:textSize="18dip"/>
			</RelativeLayout>
        </LinearLayout>	
        
        
         <LinearLayout android:id="@+id/zrtip2" 
   		 		android:background="@drawable/zrtablegroupcellfooter" 
   		 	    android:layout_width="fill_parent" 
   		 	    android:layout_height="wrap_content"
   		 	     android:paddingBottom="50dip">
   		 	<RelativeLayout android:id="@+id/RelativeLayout03"
		     	android:layout_width="fill_parent" android:layout_gravity="center"
				android:layout_height="wrap_content" android:layout_marginLeft="15.0dip"
				 android:layout_marginRight="15.0dip">		   
           		<Spinner android:id="@+id/jingzhi" android:background="@drawable/spinner_drawable" 
					android:paddingLeft="110.0dip" android:layout_width="fill_parent"
